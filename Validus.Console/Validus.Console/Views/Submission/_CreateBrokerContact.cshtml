@model  Validus.Console.DTO.CreateBrokerContactDto
<div class="row-fluid">
    <div class="span12">
        <label class="control-label">
            @Html.DisplayNameFor(model => model.NewBrokerContact)
            <abbr title="New Broker Contact is required">*</abbr>
        </label>
        @Html.TextBoxFor(model => model.NewBrokerContact, new Dictionary<string, object> 
                    { 
                        { "id", ""},
                        { "class", "span12 newBrokerContact" },
                        { "data-bind", "value: NewBrokerContact" }
                    })
        @Html.ValidationMessageFor(model => model.NewBrokerContact, null, new { @class = "help-block" })
    </div>

</div>
<div class="row-fluid">
    <div class="span12">
        <a href="#" title="Create Broker Contact" class="btn btn-primary createBrokerContact"><i class="icon-pencil icon-white"></i>Create</a>
    </div>
</div>
